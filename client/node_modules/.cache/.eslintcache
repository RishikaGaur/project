,"/home/webmob078/Desktop/1104/project/client/src/components/Home.js",["64","65"],[],"import React, { useEffect,useState } from 'react'\nimport \"./Home.css\"\nimport {useParams} from \"react-router-dom\"\nimport { useOutletContext } from 'react-router-dom'\n\nconst Home = () => {\n  const {socket}=useOutletContext();\n  console.log(socket)\n  const params=useParams();\n  const [msg,setMsg]=useState(\"\")\n  const [chat,setChat]=useState([])\n  \n  const roomId=params.roomId\n\n  useEffect(()=>{\n    if(socket){\n      return ()=>{\n        console.log(roomId)\n        socket.emit(\"join-room\",{roomId: roomId})\n      }\n    }\n  },[params])\n\n  useEffect(()=>{\n    if(socket){\n\n      return ()=>{\n        socket.on(\"receiveMsg\",(rarg)=>{\n          console.log(rarg)\n          setChat(chat => [...chat,{message:rarg,received:true}])\n          console.log(chat)\n        })\n    \n      }\n    }\n    },[socket])\n\n\n  const handleSubmit=()=>{\n    socket.emit(\"sendMsg\",msg,roomId)\n    setChat(chat => [...chat,{message:msg,received:false}])\n    setMsg(\"\")\n  }\n\n\n  const handleKeyDown=(event)=>{\n    if(event.key ===\"Enter\"){\n      handleSubmit()\n    }\n  }\n\n  return (\n    <>\n      <div className='container'>\n        <div className='child'>\n          <span className='RoomId'>{roomId}</span>\n          {\n            chat.map(n=>{\n              return <div key={n.message} className={n.received ? \"second\":\"first\"}>{n.message}</div>\n            })\n          }\n        </div>\n        <div className='ip'>\n          <input className='msg' placeholder=\"Type your message\" value={msg} onChange={(e)=>setMsg(e.target.value)} onKeyDown={handleKeyDown}></input>\n          <span className='send' onClick={handleSubmit}>Send</span>\n        </div>\n      </div>\n    </>\n  )\n}\n\nexport default Home\n","/home/webmob078/Desktop/1104/project/client/src/Router.js",[],[],"/home/webmob078/Desktop/1104/project/client/src/components/Log.js",[],[],"/home/